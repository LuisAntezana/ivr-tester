{
  "name": "@examples/google-speech",
  "version": "1.0.0",
  "private": true,
  "description": "An example of transcribing a call using Google Speech",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.0.2",
    "ngrok": "^3.2.7",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "ivr-tester": "^0.0.1"
  },
  "jest": {
    "preset": "../../jest.json"
  },
  "scripts": {
    "test:ivr": "export LOCAL_SERVER_PORT=8080 && export PUBLIC_SERVER_URL=$(curl -s localhost:4040/api/tunnels | jq -r .tunnels[0].public_url) && node --require ts-node/register src/test.ts",
    "ngrok": "ngrok http 8080",
    "build:ts": "tsc",
    "lint:prettier": "prettier --list-different 'src/**/*.ts'",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint": "yarn lint:prettier && yarn lint:eslint"
  }
}
